{
	"name": "@calmdownval/mini-dom",
	"version": "0.0.5",
	"type": "module",
	"main": "dist/index.mjs",
	"license": "MIT",
	"description": "minimal browser-style DOM implementation",
	"keywords": [
		"dom",
		"document",
		"model"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/vaclavlahuta/mini-dom.git"
	},
	"scripts": {
