		"test": "cross-env NODE_OPTIONS=\"--experimental-modules\" mocha --delay script/test.esm.js"
	},
	"devDependencies": {
		"cross-env": "^6.0.3",
		"glob": "^7.1.4",
		"mocha": "^6.2.1"
	}
